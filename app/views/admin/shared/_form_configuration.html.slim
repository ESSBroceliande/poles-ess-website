= f.form_errors

- if f.object.class.respond_to?(:enabled)
  = render "admin/shared/enabled_field", f: f

- if f.object.class.reflect_on_association(:menu_blocks)
  = render "admin/menu_blocks/nested_form", f: f

= render 'admin/seos/fields_for_form', f: f

= render "admin/shared/action_btns", f: f, index_path: index_path